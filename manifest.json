{
  "manifest_version": 2,
  "name": "Empty Project",
  "short_name": "Empty",
  "description": "Empty Chrome extencion project to make it easy to start a new project",
  "version": "0.0.1",
  "icons": {
    "128": "img/img_128.png",
    "64": "img/img_64.png",
    "48": "img/img_48.png",
    "32": "img/img_32.png",
    "24": "img/img_24.png",
    "16": "img/img_16.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "img/img_32.png",
      "24": "img/img_24.png",
      "16": "img/img_16.png"
    }
  },
  "permissions": [
    "background",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "background": {
    "scripts": ["js/bg.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/popper.js/dist/umd/popper.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "js/script.js"
      ]
    }
  ]
}
